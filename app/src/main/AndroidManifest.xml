        <activity android:name=".resetPassword" />
        <!--<activity-->
            <!--android:name=".logIn"-->
             <!--/>-->
        <activity android:name=".Login" />
        <activity android:name=".myProfile" />
        <activity android:name=".viewUser" />
        <activity android:name=".ActivityOne" />
        <activity android:name=".ActivityTwo" />
        <activity android:name=".ActivityThree" />
        <activity android:name=".AlarmNotification" />
        <activity android:name=".livePage" />
        <activity android:name=".Myplants" />
        <activity android:name=".plant1" />
        <activity android:name=".addToDatabase" />
        <activity android:name=".About" />
        <activity
